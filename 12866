#include <stdio.h>
#include <stdlib.h>

typedef enum {false, true} bool;
typedef char *str;
//

struct _element{
    struct _element *next;
    union data_{
        struct _element *array;
        float number;
        str string;
        bool boolean;
    };
    union key{
        float knumber;
        str kstring;
        bool kboolean;
    };
};
//

typedef struct _element block;
typedef union data_ data;
//

int main(){
    block start;
    data somedata;
    printf("%d,\t%d", sizeof start, sizeof somedata);
    char q = getchar();
    return 0;
}



